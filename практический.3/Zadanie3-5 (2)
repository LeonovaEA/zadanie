# -*- coding: utf-8 -*-
def Zadanie5 (number1, number2, number3):
    if number1 < number3 and number1 < number2:
        print("Число", number1, "наименьшее")
    elif number2 < number1 and number2 < number3:
        print("Число", number2, "наименьшее")
    elif number3 < number1 and number3 < number2:
        print("Число", number3, "наименьшее")
